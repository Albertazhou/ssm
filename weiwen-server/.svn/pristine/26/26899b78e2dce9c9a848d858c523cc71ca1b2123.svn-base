package com.cetc.weiwen.vo;

import java.util.List;

import com.cetc.weiwen.entity.Event;
import com.cetc.weiwen.entity.EventCompany;
import com.cetc.weiwen.entity.EventOrganization;
import com.fasterxml.jackson.annotation.JsonInclude;

@JsonInclude(JsonInclude.Include.NON_EMPTY)
public class EventReceive {

	private Event event;
	//接收上传的多媒体id
	private List<Integer> addMediaIds;
	
	//接收删除的多媒体id
	private List<Integer> delMediaIds;
	
	//接收上传的组织架构id
	private List<EventOrganization> addEventOrgList;
		
	//接收删除的组织架构id
	private List<Integer> delEventOrgIds;
	
	//接收上传的涉事公司
	private List<EventCompany> addEventCompanyIds;
	
	//接收修改的涉事公司
	private List<EventCompany> editEventCompanyIds;
	
	//接收删除的涉事公司id
	private List<Integer> delEventCompanyIds;

	public Event getEvent() {
		return event;
	}

	public void setEvent(Event event) {
		this.event = event;
	}

	public List<Integer> getAddMediaIds() {
		return addMediaIds;
	}

	public void setAddMediaIds(List<Integer> addMediaIds) {
		this.addMediaIds = addMediaIds;
	}

	public List<Integer> getDelMediaIds() {
		return delMediaIds;
	}

	public void setDelMediaIds(List<Integer> delMediaIds) {
		this.delMediaIds = delMediaIds;
	}


	public List<EventOrganization> getAddEventOrgList() {
		return addEventOrgList;
	}

	public void setAddEventOrgList(List<EventOrganization> addEventOrgList) {
		this.addEventOrgList = addEventOrgList;
	}


	public List<Integer> getDelEventOrgIds() {
		return delEventOrgIds;
	}

	public void setDelEventOrgIds(List<Integer> delEventOrgIds) {
		this.delEventOrgIds = delEventOrgIds;
	}

	public List<EventCompany> getAddEventCompanyIds() {
		return addEventCompanyIds;
	}

	public void setAddEventCompanyIds(List<EventCompany> addEventCompanyIds) {
		this.addEventCompanyIds = addEventCompanyIds;
	}

	public List<EventCompany> getEditEventCompanyIds() {
		return editEventCompanyIds;
	}

	public void setEditEventCompanyIds(List<EventCompany> editEventCompanyIds) {
		this.editEventCompanyIds = editEventCompanyIds;
	}

	public List<Integer> getDelEventCompanyIds() {
		return delEventCompanyIds;
	}

	public void setDelEventCompanyIds(List<Integer> delEventCompanyIds) {
		this.delEventCompanyIds = delEventCompanyIds;
	}
	
}
